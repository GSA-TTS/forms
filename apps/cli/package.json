{
  "name": "@gsa-tts/forms-cli",
  "version": "0.1.5",
  "description": "10x Forms Platform command-line interface",
  "type": "module",
  "license": "CC0",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsup src/* --format esm",
